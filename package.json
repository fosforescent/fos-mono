{
  "name": "@fosforescent/fos-mono",
  "version": "0.1.7",
  "scope": "@fosforescent",
  "main": "./dist/fosforescent-react.umd.js",
  "module": "./dist/fosforescent-react.mjs",
  "exports": {
    ".": {
      "browser": "./dist/frontend/index.js",
      "node": "./dist/backend/index.js",
      "require": "./dist/backend/index.js",
      "default": "./dist/frontend/index.js"
    }
  },
  "browser": {
    "net": false,
    "tls": false,
    "fs": false,
    "path": false,
    "os": false,
    "crypto": false,
    "stream": false,
    "http": false,
    "https": false,
    "zlib": false,
    "express": false,
    "express-slow-down": false,
    "express-rate-limit": false
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "serve": "vite preview",
    "build:backend": "npx vite build --mode backend",
    "build:frontend": "npx vite build --mode frontend",
    "dev:react": "vite --mode react-client",
    "dev:backend": "npx nodemon --config syc-backend/nodemon.json",
    "dev:web": "vite --mode web-client",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:combined\"",
    "test": "npx jest",
    "test:ci": "npx jest --ci",
    "setup": "npx prisma generate",
    "lint": "eslint ./**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix ./**/*.{js,jsx,ts,tsx,json}",
    "format": "prettier --write ./**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "npx test-storybook",
    "test-test-s": "wait-on tcp:6006 && npx test-storybook\"",
    "test-storybook:ci": "npx concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"storybook build --quiet && npx http-server storybook-static --port 6006 --silent\" \"npx wait-on -d 60 -i 1000 tcp:127.0.0.1:6006 && npx test-storybook\""
  },
  "prisma": {
    "seed": "tsx prisma\\seed.ts"
  },
